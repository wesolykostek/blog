type Blog = record {
    title: text;
    date: nat64;
    content: text;
    tags: vec text;
};

type Result = variant { 
    Ok : Blog; 
    Err : text; 
};

service : {
    "greet": (text) -> (text) query;
    "add_blog": (text, text, vec text) -> (Result);
    "get_blogs": () -> (vec Blog) query;
}
